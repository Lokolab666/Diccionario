
public class Nodo {
    String palabra;
    String ingles;
    String significado;
    Nodo izq;
    Nodo der;

    Nodo(String palabra, String ingles, String significado){
            this.palabra = palabra;
            this.ingles = ingles;
            this.significado = significado;
            izq = der = null;
    }	
}
